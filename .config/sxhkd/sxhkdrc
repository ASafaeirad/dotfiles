super + Return
  alacritty

super + F{2,3,4,5}
  {brave,code,pcmanfm,terminal -e mus}

super + v
  copyq show

super + ctrl + {x,m,c}
  {xkill, pavucontrol, colorpicker --short --one-shot | xsel -b}

super + shift + {space, s, e}
  {rofi -show drun, powermenu, egmenu}

super + {_, control, shift} + p
  {flameshot full -c, flameshot full -p $(xdg-user-dir PICTURES)/screenshots/, flameshot gui}

super + shift + t
  alacritty --class flyterm -e tmux attach

XF86Audio{LowerVolume,RaiseVolume,Mute}
  amixer set Master {10%-, 10%+, toggle}

XF86MonBrightness{Down,Up}
  light {-U,-A} 5



